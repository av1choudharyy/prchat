{
  "version": 2,
  "builds": [
    {
      "src": "prchat/client/package.json",
      "use": "@vercel/next" 
    },
    {
      "src": "prchat/package.json", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)", 
      "dest": "prchat/server.js" 
    },
    {
      "src": "/(.*)", 
      "dest": "prchat/client/$1"
    }
  ]
}